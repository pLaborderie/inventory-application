    "morgan": "~1.9.1",
    "nunjucks": "^3.2.2",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@prisma/cli": "^2.14.0",
    "eslint": "^7.18.0",
    "nodemon": "^2.0.7"
